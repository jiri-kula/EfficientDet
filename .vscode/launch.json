  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true,
    },
    {
      "name": "Python: Current File As Module",
      "type": "debugpy",
      "request": "launch",
      "module": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true,
    },
    {
      "name": "train",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/train.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "env": {
        // "CUDA_VISIBLE_DEVICES": "",
        "PATH": "/usr/local/cuda-12.2/bin"
      },
    },
    {
      "name": "profile train",
      "type": "python",
      "request": "launch",
      "module": "cProfile",
      "console": "integratedTerminal",
      "justMyCode": true,
      "args": [
        "-o",
        "/tmp/tmp.prof",
        "${workspaceFolder}/train.py"
      ]
    },
    {
      "name": "test_dataset",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/test_dataset.py",
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "inference",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/inference.py",
      "args": [
        "-w",
        "checkpoints/tfrecord_allrot/weights-16",
        "-n",
        "efficientdet_d0",
        "-i",
